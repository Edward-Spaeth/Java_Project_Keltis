<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="imageview" prefHeight="678.0" prefWidth="957.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.keltisproject.KeltisController">
   <children>
      <ImageView fx:id="imageView" fitHeight="770.0" fitWidth="976.0" layoutX="-17.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@WhatsApp Bild 2023-01-11 um 13.02.15.jpg" />
         </image>
      </ImageView>
      <VBox layoutX="431.0" layoutY="330.0" prefHeight="85.0" prefWidth="118.0">
         <children>
            <Button mnemonicParsing="false" onMouseClicked="#switchToStartScene" prefHeight="28.0" prefWidth="118.0" text="Start">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Button mnemonicParsing="false" onMouseClicked="#switchToSettingsScene" prefHeight="28.0" prefWidth="118.0" text="Settings">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Button mnemonicParsing="false" onMouseClicked="#exitprogram" prefHeight="23.0" prefWidth="118.0" text="Exit">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
